<addons>
<addon id="plugin.video.Real.stream"
       name="[COLOR fuchsia]Real Stream[/COLOR]"
       version="0.5.5"
       provider-name="Netai">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
	<import addon="plugin.program.favoritos-realstream" version="1.0.2"/>
    <import addon="script.module.urlresolver"  version="5.0.33"/>
	<import addon="script.module.resolveurl"  version="5.0.27"/>
    <import addon="script.extendedinfo" optional="5.6.0"/>
    <import addon="plugin.video.youtube" />

  </requires>
  <extension point="xbmc.python.pluginsource" library="run.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="es">Realstream</summary>
    <language>es</language>
    <description lang="es">>Tu libreria de peliculas online.
	</description>
     <platform>all</platform>
  </extension>
</addon>

