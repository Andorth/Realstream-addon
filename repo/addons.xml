<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.program.real.stream.fav" name="Real Stream Favoritos" version="1.0.1" provider-name="spoyser/Netai">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
	<import addon="script.module.metahandler" optional="true" />
  </requires>

  <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>executable</provides>
  </extension>

  <extension point="xbmc.service" library="service.py" start="startup" />

  <extension point="xbmc.addon.metadata">
    <summary lang="es"></summary>
    <description lang="es">Addon Super favorites modificado para su uso con Real Stream addon. Ten clasificadas sus peliculas creando acceso directo a la misma para luego poderla reproducir. Agradecimentos a los creadores del Programa Addon Super Favorites.</description>
    <platform>all</platform>
    <language>en</language>
    <license>GNU GENERAL PUBLIC LICENSE. Version 2, June 1991</license>

    <website></website>
    <source></source>
    <email></email>
  </extension>

  <extension point="kodi.context.item" library="capture.py">
      <item>
        <label>30000</label>
        <visible>true</visible>
      </item>
    </extension>
</addon>

<addon id="plugin.program.videoteca" version="0.0.1" name="Videoteca" provider-name="Netai">
    <requires>
        <import addon="xbmc.python" version="2.1.0"/>
        <import addon="script.module.urlresolver"/>
        <import addon="script.module.echo"/>
    </requires>
    <extension point="xbmc.python.module" library="lib" />
	<extension point="xbmc.python.pluginsource" library="default.py" />
	<extension point="xbmc.addon.metadata">

    <provides>executable</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>Tu videoteca</summary>
        <description>Gestiona tu libreria de enlaces a servidores publicos de internet, y reproducelos desde tu media center.</description>
        <platform>all</platform>
	</extension>
</addon>

<addon id="plugin.video.Real.stream"
       name="Real Stream"
       version="0.2.0"
       provider-name="Netai">

<requires>
	<import addon="xbmc.python" version="2.1.0"/>
	<import addon="script.module.urlresolver" version="2.2.0"/>
	<import addon="plugin.program.real.stream.fav" version="1.0.0"/>
	<import addon="plugin.program.videoteca" version="0.0.1"/>
	<import addon="metadata.common.fanart.tv" optional="true"/>
    <import addon="script.module.echo"/ optional="true"/>

</requires>
 <extension point="xbmc.python.pluginsource"
            library="run.py">
        <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="es">Libreria de tus peliculas.</summary>
    <language>es</language>
    <description lang="es">Libreria de peliculas de la nube en tu Kodi.</description>
     <platform>all</platform>
  </extension>
</addon>

