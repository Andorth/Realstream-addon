<addons>
<addon id="plugin.program.favoritos-realstream" name="[COLOR pink]Favoritos[/COLOR] [COLOR gold]Real Stream[/COLOR]" version="1.0.5" provider-name="spoyser">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
	<import addon="script.module.metahandler" optional="true" />
  </requires>

  <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>executable</provides>
  </extension>

  <extension point="xbmc.service" library="service.py" start="startup" />

  <extension point="xbmc.addon.metadata">
    <summary lang="es"></summary>
    <description lang="en">Addon Super favorites modificado para su uso con Real Stream addon. Ten clasificadas sus peliculas creando acceso directo a la misma para luego poderla reproducir. Agradecimentos a los creadores del Programa Addon Super Favorites.</description>
    <platform>all</platform>
    <language>en</language>
    <license>GNU GENERAL PUBLIC LICENSE. Version 2, June 1991</license>

    <website></website>
    <source></source>
    <email></email>
  </extension>

  <extension point="kodi.context.item" library="capture.py">
      <item>
        <label>30000</label>
        <visible>true</visible>
      </item>
    </extension>
</addon>

<addon id="plugin.video.Real.stream"
       name="[COLOR orange]Real Stream[/COLOR]"
       version="0.4.3"
       provider-name="Netai">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
    <import addon="script.module.simple.downloader" version="1.9.5"/>
	<import addon="plugin.program.favoritos-realstream" version="1.0.2"/>
    <import addon="script.module.urlresolver" optional="true"/>
    <import addon="script.extendedinfo" optional="true"/>
	<import addon="script.module.youtube.dl" optional="true"/>
    <import addon="plugin.video.youtube" />

  </requires>
  <extension point="xbmc.python.pluginsource" library="run.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="es">Libreria de tus peliculas.</summary>
    <language>es</language>
    <description lang="es">Visiona peliculas procedentes de internet en tu dispositivo. Este script no aloja contenido protegido en su interior. Es totalmente gratuito.</description>
     <platform>all</platform>
  </extension>
</addon>

