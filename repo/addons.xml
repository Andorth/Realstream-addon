<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.video.Real.stream"
       name="Real Stream"
       version="0.2.0"
       provider-name="Netai">

<requires>
	<import addon="xbmc.python" version="2.1.0"/>
	<import addon="script.module.urlresolver" optional="true" version="2.2.0"/>
	<import addon="plugin.program.real.stream.fav" optional="true" version="1.0.0"/>
	<import addon="plugin.program.videoteca" optional="true" version="0.0.1"/>
</requires>
 <extension point="xbmc.python.pluginsource"
            library="run.py">
        <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="es">Libreria de tus peliculas.</summary>
    <language>es</language>
    <description lang="es">Libreria de peliculas de la nube en tu Kodi.</description>
     <platform>all</platform>
  </extension>
</addon>

<addon id="repository.Realstream" name="[COLOR orange]Realstream[/COLOR] Repo" version="1.2.1" provider-name="Netai">

    <extension point="xbmc.addon.repository" name="Realstream-repo">
		<info>https://raw.githubusercontent.com/Andorth/Realstream-addon/master/repo/addons.xml</info>
		<checksum>https://raw.githubusercontent.com/Andorth/Realstream-addon/master/repo/addons.xml.md5</checksum>
		<datadir zip="true">https://raw.githubusercontent.com/Andorth/Realstream-addon/master/repo/</datadir>
	</extension>
    <extension point="xbmc.addon.metadata">
        <summary>Repositorio Real Stream</summary>
        <description>Soporte del addon Real Stream.</description>
        <disclaimer>Este addon es gratuito, no pagues por el.</disclaimer>
        <platform>all</platform>
    </extension>
</addon>

